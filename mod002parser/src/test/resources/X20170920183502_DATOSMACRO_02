}			
function shFlgs(id){
ga('send', 'event', 'button', 'click', 'flags-continent');
if (id=='world'){
	jQuery('.flg').css({ "display": 'inline-block'});
}else {
jQuery('.flg').css({ "display": 'none'});
jQuery('.flg-'+id).css({ "display": 'inline-block'});
}}
</script>
