        f = document.forms.aspnetForm;
        sel = [ f.ctl00_Contenido_SelMercado, f.ctl00_Contenido_SelÍndice, f.ctl00_Contenido_SelSector ];
    });
    var rstSel = function (n) {
        if (sel[n].selectedIndex > 0) {
            for (var i=0; i<3; ++i)
                if (n!=i) sel[i].selectedIndex = 0;
        };
    };
    var chkSel = function () {
        for (var i=0; i<3; ++i)
            if (sel[i].selectedIndex > 0) return true;
        return false;
    }
</script>
